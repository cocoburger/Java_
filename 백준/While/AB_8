package com.back.sum;

import java.util.Scanner;

public class AB_8 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		int A = sc.nextInt();

		for(int i = 1; i <= A; i++) {
			int c = sc.nextInt();
			int d = sc.nextInt();

			System.out.println("Case # " + i + " : " + c + " + " +  d + " = " + (c+d));

		}
		sc.close();
	}
}

